all:
  vars:
    ansible_ssh_private_key_file: .${keypath}
    ansible_user: ${user}

controlplanes:
  hosts:
%{ for host in control_planes ~}
    ${host.hostname}:
      ansible_host: ${host.ip}
%{ endfor ~}
nodes:
  hosts:
%{ for host in nodes ~}
    ${host.hostname}:
      ansible_host: ${host.ip}
%{ endfor ~}
